  (comment "FINISH ME")
  (let [raw_rooms (clojure.string/split-lines (slurp input_file))]
    (let [split_rooms (mapv clojure.string/split raw_rooms (repeatedly (fn [] #"\|")))]
      (let [split_exits (mapv exit-splitter split_rooms)]
        split_rooms))))

